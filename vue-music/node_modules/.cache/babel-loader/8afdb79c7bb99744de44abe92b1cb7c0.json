,CAAkB,WAAU,IAA5B;AACF;AAVO;AA1BI,CAAf","sourcesContent":["<template>\r\n  <div class=\"popular_playlist\">\r\n    <div class=\"playlisttop\">\r\n      <h1>热门歌单</h1>  \r\n      <ul>\r\n        <li :class=\"{current:isactive}\" v-for=\"item of ctitles.slice(0, 5)\" :key=\"item.id\" @click=\"poplistclick(item.name)\">\r\n          <a href=\"#\">{{item.name}}</a>\r\n        </li>\r\n      </ul>\r\n      <span @click=\"MoreClick(currentName)\">更多</span>\r\n    </div>\r\n    <play-list>\r\n      <play-list-item :cplaylistitem=\"item\" v-for=\"item of cplaylists.slice(0, 5)\" :key=\"item.id\"></play-list-item>\r\n    </play-list>\r\n  </div>  \r\n</template>\r\n\r\n<script>\r\nimport PlayList from './PlayList.vue'\r\nimport PlayListItem from './PlayListItem.vue'\r\nexport default {\r\n  components: { \r\n    PlayList, \r\n    PlayListItem \r\n  },\r\n  name:\"popplaylist\",\r\n  data() {\r\n    return {\r\n      isactive:true,\r\n      currentName:''\r\n    }\r\n  },\r\n  props:{\r\n    ctitles:{\r\n      type:Array,\r\n      default(){\r\n        return []\r\n      }\r\n    },\r\n    cplaylists:{\r\n      type:Array,\r\n      default(){\r\n        return []\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    poplistclick(name){\r\n      this.$emit('poplistclick',name)\r\n      this.currentName = name;\r\n      this.isactive = !this.isactive;\r\n    },\r\n    //点击更多\r\n    MoreClick(name){\r\n      console.log(name);\r\n      this.$router.push('/more/'+ name)\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n<style>\r\n.playlisttop{\r\n  display: flex;\r\n  height: 44px;\r\n  line-height: 44px;\r\n  text-align: center;\r\n}\r\n.popular_playlist h1::before{\r\n  content: \"\";\r\n  display:inline-block;\r\n  width: 40px;\r\n  height: 40px;\r\n  vertical-align:middle;\r\n  background-image: url(../../../assets/img/index.png);\r\n  background-position:193px 313px;\r\n}\r\n.popular_playlist span{\r\n  margin-left: auto;\r\n  font-size: 15px;\r\n  cursor: pointer;\r\n  padding-right: 1