B,KAAA,CAAA,QAAA,CAAS,IAAW,CAA1B,EAAmB;AAAA;AAAnB,GAoBI,EAnBJ,mBAAA,CAIM,KAJN,EAAA,UAAA,EAIM,CAHJ,mBAAA,CAA8C,KAA9C,EAA8C;AAAxC,IAAA,GAAG,EAAE,KAAA,CAAA,QAAA,CAAS,OAAT,CAAiB,SAAkB;AAAP,IAAA,GAAG,EAAC;AAAG,GAA9C,E,IAAA,E;;AAAA,I,UAAA,CAGI,EAFJ,mBAAA,CAA0D,GAA1D,EAAA,UAAA,EAA0D,CAA9C,mBAAA,CAA0C,MAA1C,EAA0C,IAA1C,EAA0C,gBAAA,CAAlC,KAAA,CAAA,QAAA,CAAS,OAAT,CAAiB,QAAiB,CAA1C,EAAiC;AAAA;AAAjC,GAA8C,CAA1D,CAEI,EADJ,mBAAA,CAAkD,MAAlD,EAAkD,IAAlD,EAAM,UAAK,gBAAA,CAAE,QAAA,CAAA,OAAA,CAAQ,KAAA,CAAA,QAAA,CAAS,UAAjB,CAAF,CAAX,EAAwC;AAAA;AAAxC,GACI,CAJN,CAmBI,EAdJ,mBAAA,CAKM,KALN,EAAA,UAAA,EAKM,CAJJ,YAAA,CAAwC,oBAAxC,EAAwC;AAA7B,IAAA,IAAI,EAAC;AAAwB,GAAxC,EAAyB;sBAAC;AAAA,aAAE,C,UAAA,CAAF;AAAA,K,CAAD;;;;AAAA,GAAzB,CAII,EAHJ,YAAA,CAAsE,oBAAtE,EAAsE;AAA3D,IAAA,IAAI,EAAC;AAAsD,GAAtE,EAAyB;sBAAC;AAAA,aAAG,C,iBAAH,QAAG,gBAAA,CAAE,KAAA,CAAA,WAAA,CAAY,YAAd,CAAH,GAA+B,G,EAAC;AAAA;OAA7B,CAAH;AAAA,K,CAAD;;;;AAAA,GAAzB,CAGI,EAFJ,YAAA,CAAoE,oBAApE,EAAoE;AAAzD,IAAA,IAAI,EAAC;AAAoD,GAApE,EAAyB;sBAAC;AAAA,aAAG,C,iBAAH,QAAG,gBAAA,CAAE,KAAA,CAAA,WAAA,CAAY,UAAd,CAAH,GAA6B,G,EAAC;AAAA;OAA3B,CAAH;AAAA,K,CAAD;;;;AAAA,GAAzB,CAEI,EADJ,YAAA,CAAsE,oBAAtE,EAAsE;AAA3D,IAAA,IAAI,EAAC;AAAsD,GAAtE,EAAyB;sBAAC;AAAA,aAAG,C,iBAAH,QAAG,gBAAA,CAAE,KAAA,CAAA,WAAA,CAAY,YAAd,CAAH,GAA+B,G,EAAC;AAAA;OAA7B,CAAH;AAAA,K,CAAD;;;;AAAA,GAAzB,CACI,CALN,CAcI,EARJ,mBAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFJ,WAEI,G,kBADJ,mBAAA,CAAkE,SAAlE,EAAkE,IAAlE,EAAkE,WAAA,CAAxC,KAAA,CAAA,QAAA,CAAS,IAA+B,EAA3B,UAA5B,IAA4B,EAAvB,KAAuB,EAAlB;yBAArB,mBAAA,CAAkE,GAAlE,EAAkE;AAAxB,MAAA,GAAG,EAAE;AAAmB,KAAlE,EAAoD,gBAAA,CAAI,IAAJ,CAApD,EAA4D;AAAA;AAA5D,K;GAAkE,CAAlE,E;;AAAA,GACI,EAHN,CAQI,EAJJ,mBAAA,CAAA,oHAAA,CAII,CAtBN,CAyBI,EAFJ,YAAA,CAA8D,0BAA9D,EAA8D;AAA5C,IAAA,OAAO,EAAE,KAAA,CAAA,QAAA,CAAS;AAA0B,GAA9D,E,IAAA,E;;AAAA,I,WAAA,CAEI,EADH,YAAA,CAA6C,kBAA7C,EAA6C;AAAnC,IAAA,WAAW,EAAE,KAAA,CAAA;AAAsB,GAA7C,EAAiC,IAAjC,EAAiC;AAAA;AAAjC,IAAiC,CAAA,aAAA,CAAjC,CACG,CA1BN,C","sourcesContent":["<template>\r\n  <div class=\"playDetail\" v-if=\"playData.creator\">\r\n    <div class=\"basemessage\">\r\n      <img :src=\"playData.coverImgUrl\" alt=\"\">\r\n      <h2>{{playData.name}}</h2>\r\n      <div class=\"creator\">\r\n        <img :src=\"playData.creator.avatarUrl\" alt=\"\">\r\n        <a href=\"#\"><span>{{playData.creator.nickname}}</span></a>\r\n        <span>创建时间：{{getTime(playData.createTime)}}</span>\r\n      </div>\r\n      <div class=\"PlayCollect\">\r\n        <el-button type=\"primary\">播放</el-button>\r\n        <el-button type=\"primary\">收藏({{playCollect.commentCount}})</el-button>\r\n        <el-button type=\"primary\">分享({{playCollect.shareCount}})</el-button>\r\n        <el-button type=\"primary\">评论({{playCollect.commentCount}})</el-button>\r\n      </div>\r\n      <div class=\"label\">\r\n        <b>标签：</b>\r\n        <i v-for=\"(item,index) in playData.tags\" :key=\"index\">{{item}}</i>\r\n      </div>\r\n      <!-- <div class=\"introduce\">\r\n        <b>介绍：</b>\r\n        <span>{{playData.description}}</span>\r\n      </div> -->\r\n    </div>\r\n    <songdisplaylist :ctracks=\"playData.tracks\"></songdisplaylist>\r\n     <comment :ccommitlist=\"commitlist\"></comment>\r\n  </div>  \r\n</template>\r\n\r\n<script>import songdisplaylist from '../../../components/common/BaseDisplay/SongDisplayList.vue';\r\nimport comment from '../../../components/common/Comment/Comment.vue'\r\nimport {getOneList} from \"../../../network/home\"\r\nimport {getPlayCollect,getPlayCommit} from \"../../../network/detail\"\r\nexport default {\r\n  name:\"playdetail\",\r\n  components:{\r\n    songdisplaylist,\r\n    comment\r\n  },\r\n  data() {\r\n    return {\r\n      playData:{},\r\n      playCollect:{},\r\n      commitlist:{}\r\n    }\r\n  },\r\n  computed: {\r\n    id() {\r\n      return this.$route.params.id;\r\n    },\r\n  },\r\n  mounted() {\r\n    this.getDetail(this.id);\r\n    this.getCollect(this.id);\r\n    this.getComment(this.id);\r\n  },\r\n  methods: {\r\n    getDetail(id){\r\n      getOneList(id).then(res=>{\r\n        this.playData = res.data.playlist;\r\n        // console.log(this.playData);\r\n      })\r\n    },\r\n     getCollect(id){\r\n      getPlayCollect(id).then(res=>{\r\n        this.playCollect = res.data;\r\n      })\r\n    },\r\n     getComment(id){\r\n      getPlayCommit(id).then(res=>{\r\n        // console.log(res);\r\n         this.commitlist = res.data;\r\n      })\r\n    },\r\n     getTime(dt){\r\n      var time = new Date(dt);\r\n      var y = time.getFullYear();\r\n      var m = time.getMonth()+1;\r\n      var d = time.getDate();\r\n      return y+ '-' + (m<10? '0' +m:m) + '-' +(d<10? '0' +d:d);\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n<style>\r\n.playDetail{\r\n  position: relative;\r\n  margin-top: 50px;\r\n}\r\n.basemessage{\r\n  height: 200px;\r\n  font-size: 14px;\r\n}\r\n.basemessage>img{\r\n  float: left;\r\n  /* width: 100px; */\r\n  height: 100%;\r\n  margin-right: 20px; \r\n  box-shadow: 9px 3px 9px -1px rgba(0, 0,0,.4);\r\n}\r\n.basemessage h2{\r\n    margin: 0 0 12px;\r\n}\r\n.basemessage h2::before{\r\n  content: \"\";\r\n  display:inline-block;\r\n  width: 60px;\r\n  height: 30px;\r\n  vertical-align:middle;\r\n  background-image: url(../../../assets/img/icon.png);\r\n  background-position:0px -239px;\r\n}\r\n.creator{\r\n  margin: 0 0 20px;\r\n    line-height: 35px;\r\n}\r\n.creator img{\r\n      float: left;\r\n    width: 45px;\r\n    height: 45px;\r\n    margin-right: 10px;\r\n}\r\n.creator span{\r\n  margin-right: 10px;\r\n}\r\n.label{\r\n  margin: 25px 0 5px;\r\n    line-height: 22px;\r\n}\r\n.label i{\r\n  cursor: pointer;\r\n    zoom: 1;\r\n    padding: 5px 11px 5px 11px;\r\n    background-position: 0 0;\r\n    margin-right: 5px;\r\n    border: 1px solid rgb(180, 180, 180);\r\n    border-radius: 10px;\r\n}\r\n.PlayCollect{\r\n  margin-top: 20px;\r\n}\r\n</style>"],"sourceRoot":""}]}