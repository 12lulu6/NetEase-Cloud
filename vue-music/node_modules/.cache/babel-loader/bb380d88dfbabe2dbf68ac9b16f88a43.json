n    }\r\n  },\r\n  methods: {\r\n    poplistclick(name){\r\n      this.$emit('poplistclick',name)\r\n      this.currentName = name;\r\n      this.isactive = !this.isactive;\r\n    },\r\n    //点击更多\r\n    MoreClick(name){\r\n      console.log(name);\r\n      this.$router.push('/more/'+ name)\r\n    }\r\n  },\r\n}\r\n</script>\r\n\r\n<style>\r\n.playlisttop{\r\n  display: flex;\r\n  height: 44px;\r\n  line-height: 44px;\r\n  text-align: center;\r\n}\r\n.popular_playlist h1::before{\r\n  content: \"\";\r\n  display:inline-block;\r\n  width: 40px;\r\n  height: 40px;\r\n  vertical-align:middle;\r\n  background-image: url(../../../assets/img/index.png);\r\n  background-position:193px 313px;\r\n}\r\n.popular_playlist span{\r\n  margin-left: auto;\r\n  font-size: 15px;\r\n  cursor: pointer;\r\n  padding-right: 18px;\r\n}\r\n.popular_playlist span::after{\r\n  content: \"\";\r\n  display:inline-block;\r\n  width: 20px;\r\n  height: 20px;\r\n  vertical-align:middle;\r\n  background-image: url(../../../assets/img/index.png);\r\n  background-position: 5px 229px;\r\n}\r\n.popular_playlist h1{\r\n  margin: 0;\r\n}\r\n.popular_playlist ul{\r\n  box-sizing: content-box;\r\n  text-align: center;\r\n  margin-left: 20px;\r\n  font-size: 14px;\r\n}\r\n.popular_playlist ul li{\r\n  float: left;\r\n  cursor: pointer;\r\n}\r\n.popular_playlist ul li a{\r\n  padding:10px 15px;\r\n}\r\n.popular_playlist ul li:hover a{\r\n  color: #ea986c;\r\n  border-bottom: 2px solid #ea986c;\r\n}\r\n.current{\r\n  color:#ea986c;\r\n}\r\n</style>"],"sourceRoot":""}]}